<form action="/staffs/edit/<%= data.id %>" method="post">
    <label for="firstName">First Name</label><br>
    <input type="text" name="firstName" id="firstName" value="<%= data.firstName %>"><br>
    <label for="lastName">Last Name</label><br>
    <input type="text" name="lastName" id="lastName" value="<%= data.lastName %>"><br>
    <label for="email">Email</label><br>
    <input type="email" name="email" id="email" value="<%= data.email %>"><br>
    <label for="phone">Phone Number</label><br>
    <input type="text" name="phone" id="phone" value="<%= data.phone %>"><br>
    <label for="gender">Gender</label><br>
    <select name="gender" id="gender">
        <% if(data.gender === 'Male') { %>
        <option value="Male" selected>Male</option>
        <option value="Female">Female</option>
        <% } else { %>
        <option value="Male">Male</option>
        <option value="Female" selected>Female</option>
        <% } %>
    </select><br>
    <label for="age">Age</label><br>
    <input type="text" name="age" id="age" value="<%= data.age %>" ><br>
    <button type="submit">Edit Staff</button>
</form>